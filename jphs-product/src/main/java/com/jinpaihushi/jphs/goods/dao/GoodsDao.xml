	<select id="getGoodsByPricePart" resultMap="goods">
		SELECT
		g.*
		FROM
		price_part pp
		LEFT JOIN price p ON pp.price_id = p.id
		LEFT JOIN goods g
		ON p.goods_id =g.id
		WHERE pp.id = #{pricePartId}
	</select>
	
	
	<select id="getAllGoods" resultMap="goodsInfo">
		SELECT
			jg.goods_id goodsId,
			pp.id pp_id,
			g.title,
			p.grade_name,
			p.service_number,
			pp.price,
			pp.max_price,
			p.grade
		FROM
			jobtitle_goods jg
		LEFT JOIN price p ON (jg.goods_id = p.goods_id)
		LEFT JOIN price_part pp ON (pp.price_id = p.id)
		LEFT JOIN goods g ON(jg.goods_id = g.id)
		WHERE
			1 = 1
		AND pp.site_id = '3790272c-0ad4-445d-9fe6-d1710f1b7e06'
		AND pp.`status` = 1
		AND jg.`status` = 1
		AND jg.jobtitle_id IN (
			SELECT  j.id from jobtitle j
			 WHERE j.jobtitle_type_id in(
					SELECT
						
						j.jobtitle_type_id
					FROM
						nurse_jobtitle nj
					LEFT JOIN jobtitle j ON (nj.jobtitle_id = j.id)
					WHERE
						nj.creator_id =  #{creatorId}
					AND nj.`status` = 1
			)
		)
		AND pp.id NOT IN (
	SELECT pn.price_part_id FROM price_nurse pn WHERE pn.creator_id =  #{creatorId} and pn.`status` = 0
)
	AND jg.goods_id NOT IN( 
	SELECT g.id FROM goods g 
		LEFT JOIN product p ON (p.id = g.product_id) 
		WHERE p.id = 139
)
		GROUP BY pp.id 
		ORDER BY
			jg.goods_id,
			p.grade_name,
			p.service_number,
			p.grade
	</select>
	
	<select id="getMyService" resultMap="goodsInfo">
		SELECT
			jg.goods_id goodsId,
			pp.id pp_id,
			g.title,
			p.grade_name,
			p.service_number,
			pp.price,
			pp.max_price,
			p.grade,
			si.url,
			pn.price pnPrice
			
		FROM
			jobtitle_goods jg
		LEFT JOIN price p ON (jg.goods_id = p.goods_id)
		LEFT JOIN price_part pp ON (pp.price_id = p.id)
		LEFT JOIN goods g ON (jg.goods_id = g.id)
		LEFT JOIN service_images si ON (jg.goods_id = si.source_id)
		LEFT JOIN price_nurse pn ON (pn.price_part_id = pp.id)
		WHERE
			1 = 1
		AND pp.site_id = '3790272c-0ad4-445d-9fe6-d1710f1b7e06'
		AND pp.`status` = 1
		AND jg.`status` = 1
		AND pn.`status` = 0
		AND si.device_type = 2
		AND jg.jobtitle_id IN (
			SELECT
				nj.jobtitle_id
			FROM
				nurse_jobtitle nj
			WHERE
				nj.creator_id = #{creatorId}
		)
		AND pn.creator_id = #{creatorId}
		ORDER BY
			jg.goods_id,
			p.grade_name,
			p.service_number
	</select>
	<select id="getGoodsByProduct" resultType="java.util.HashMap">
		SELECT DISTINCT
			g.id,
			g.title
		FROM
			platform_goods pg
		LEFT JOIN goods g ON pg.goods_id = g.id
		WHERE
			pg.`status` = 1
		AND g.product_id = #{productId}
		AND pg.platform_id = #{platformId}
	</select>
