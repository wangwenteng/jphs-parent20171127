	<select id="getGoodsDetail" resultMap="goodsDetail">
		SELECT
		s.id,
		s.title,
		s.sub_title,
		s.product_id,
		s.content,
		s.dz_tool,
		s.hl_tool,
		s.insurance,
		s.tijian_type_id,
		s.tijian_hospital_id,
		s.must,
		s.type,
		s.grade_type,
		s.sort,
		s.is_bg,
		s.is_prove,
		s.is_pz,
		s.is_rr,
		s.status,
		s.remark,
		s.create_time,
		s.creator_id,
		s.creator_name,
		i.id service_id,
		i.source_id,
		i.url,
		i.path,
		i.device_type,
		i.type
		service_type,
		i.sort service_sort,
		i.remarks
		service_remark,
		i.status
		service_status,
		i.create_time
		service_create_time,
		i.creator_id
		service_creator_id,
		i.creator_name
		service_creator_name,
		p.id
		product_id,
		p.title product_title,
		p.sort
		product_sort,
		p.status
		product_status,
		p.remark product_remark
		FROM
		goods s
		LEFT JOIN
		`service_images` i ON i.source_id = s.id
		LEFT JOIN `product` p
		ON p.id
		= s.product_id
		WHERE s.id = #{id} ORDER BY s.sort;
	</select>

	<select id="getColumnGoods" resultType="java.util.HashMap">
		SELECT
		s.id,
		s.title,
		s.sub_title,
		s.product_id,
		s.content,
		s.dz_tool,
		s.hl_tool,
		s.is_pz,
		s.is_rr,
		s.insurance,
		IFNULL(s.tijian_type_id,'') tijian_type_id,
		s.tijian_hospital_id,
		s.must,
		s.type,
		s.grade_type,
		s.sort,
		IFNULL(s.is_bg,'') is_bg,
		IFNULL(s.is_prove,'') is_prove,
		s.STATUS,
		IFNULL(s.remark,'') remark,
		s.create_time,
		s.creator_id,
		s.creator_name,
		i.id service_id,
		i.source_id,
		i.url,
		i.path,
		i.device_type,
		i.type
		service_type,
		i.sort service_sort,
		i.remarks service_remark,
		i. STATUS
		service_status,
		i.create_time service_create_time,
		i.creator_id
		service_creator_id,
		i.creator_name service_creator_name,
		p.id
		product_id,
		p.title product_title,
		p.sort product_sort,
		p. STATUS
		product_status,
		p.remark product_remark,
		t.price,
		t.service_number,
		t.service_time,
		t.unit,
		t.grade_name,
		t.old_price
		<!-- ,(SELECT COUNT(id)
		numberLen FROM order_goods og WHERE og.goods_id = s.id ) numberLen -->
		FROM goods s
		LEFT JOIN `service_images` i ON i.source_id = s.id
		LEFT
		JOIN `product` p ON p.id = s.product_id
		LEFT JOIN ( SELECT p.goods_id
		id, pp.price price,
		pp.old_price old_price,
		p.service_number,
		p.service_time,
		p.grade_name,
		p.unit
		FROM
		price p
		LEFT JOIN price_part pp ON p.id = pp.price_id
		WHERE
		pp.site_id
		=#{siteId} ORDER BY pp.price ) t ON t.id = s.id
		WHERE
		s.id IN ( SELECT
		link FROM column_service CE WHERE CE.column_id = #{columnId} )
		AND
		i.device_type =#{deviceType} AND s.STATUS = 1 AND p.STATUS = 1 GROUP
		BY s.id ORDER BY s.sort;
	</select>

