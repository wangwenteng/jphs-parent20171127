		SELECT
		<include refid="field" />
		FROM `TIJIAN_HEALTH_LOG` THL
		<include refid="dynamicWhere" />
		<include refid="orderby" />
	</select>

	<select id="query" resultMap="tijianHealthLog">
		SELECT
		<include refid="field" />
		FROM `TIJIAN_HEALTH_LOG` THL
		<include refid="dynamicWhere" />
		<include refid="orderby" />
	</select>

	<select id="loadById" resultMap="tijianHealthLog">
		SELECT
		<include refid="field" />
		FROM `TIJIAN_HEALTH_LOG` THL
		WHERE THL.ID = #{id}
	</select>
	<select id="getUserHealthLog" resultType="java.util.HashMap">
		SELECT
		thl.id,
		thl.user_id userId,
		thl.`name` userName,
		u.sex sex,
		(SELECT url FROM
		nurse_images ni WHERE ni.source_id =u.id AND ni.type =1) headPicture,
		thl.health_time,
		DATE_FORMAT(IFNULL(o.accept_time, ''),'%Y-%m-%d') accept_time,
		IFNULL(oo.hospital,'') hospital,
		IFNULL(og.title,'')  title,
		<if test="id!=null and id!=''">
			IFNULL(o.accept_user_id, '') nurseId,
			IFNULL(ua.`name`, '') nurseName,
			IFNULL(ua.phone,'') nursePhone,
			IFNULL(thl.image,'') image,
		</if>
		CASE u.birthday
		WHEN u.birthday IS  NULL THEN
		'0'
		ELSE
				
				(
					YEAR(now()) - YEAR(str_to_date(u.birthday,'%Y-%m')) - 1
				)
			+ (
					DATE_FORMAT(str_to_date(u.birthday,'%Y-%m'), '%m%d') &lt;= DATE_FORMAT(NOW(), '%m%d')
				)
		END AS age
		FROM
		tijian_health_log thl
		LEFT JOIN `user` u ON
		thl.user_id = u.id
		LEFT JOIN order_goods og ON thl.order_id = og.order_id
		LEFT JOIN goods g ON og.goods_id = g.id
		LEFT JOIN `order` o ON thl.order_id = o.id
		LEFT JOIN `user`
			ua ON o.accept_user_id = ua.id
		LEFT JOIN order_other oo ON o.id =
		oo.order_id
		<where>
			AND thl.status =1 AND thl.user_id = #{userId}
			<if test="id!=null and id!=''">
				AND thl.id =#{id}
			</if>
		</where>
		ORDER BY thl.create_time DESC
	</select>
</mapper>
