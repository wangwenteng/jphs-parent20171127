<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">

<tiles-definitions>

	<definition name="default.layout" template="/WEB-INF/tiles/layout/layout.jsp">
		<put-attribute type="string" name="title" value="" />
		<put-attribute type="template" name="header" value="/WEB-INF/tiles/common/header.jsp" />
		<put-attribute type="template" name="left" value="/WEB-INF/tiles/common/left.jsp" />
		<put-attribute type="template" name="body" value="/WEB-INF/tiles/common/body.jsp" />
		<put-attribute type="template" name="footer" value="/WEB-INF/tiles/common/footer.jsp" />
				
		<put-list-attribute name="css" inherit="true">
			<add-attribute value="/static/css/common.css" />					
		</put-list-attribute>
		
		<put-list-attribute name="scripts" inherit="true">
			<!-- <add-attribute value="/static/js/jquery-3.2.1.js" />		 -->				
		</put-list-attribute> 
	</definition>
	
	<definition name="empty.layout" template="/WEB-INF/tiles/layout/layout-empty.jsp">
		<put-attribute type="string" name="title" value="" />
				
		<put-list-attribute name="css" inherit="true">
			<add-attribute value="/static/css/common.css" />	
		</put-list-attribute>
		
		<put-list-attribute name="scripts" inherit="true">			
<!-- 			<add-attribute value="/static/js/echarts.js" />
			<add-attribute value="/static/js/echarts.min.js" />			

			
 -->		</put-list-attribute> 
	</definition>	
	
</tiles-definitions>
