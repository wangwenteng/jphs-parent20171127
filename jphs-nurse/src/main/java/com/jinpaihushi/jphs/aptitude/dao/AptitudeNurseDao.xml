	<sql id="condition">
		<if test="id!=null and id!=''">
			AND AN.id=#{id}
		</if>
		<if test="aptitudeId!=null and aptitudeId!=''">
			AND AN.aptitude_id=#{aptitudeId}
		</if>
		<if test="nurseId!=null and nurseId!=''">
