           <if test="creatorName!=null">
				NA.creator_name=#{creatorName}, 
		   </if>
           <if test="createTime!=null">
				NA.create_time=#{createTime}, 
		   </if>
           <if test="status!=null">
				NA.status=#{status}, 
		   </if>
		</set>
	</sql>
	
	<sql id="orderby">
		<if test="orderby != null">
			ORDER BY ${orderby}
		</if>
	</sql>

	<resultMap id="nurseActive" type="com.jinpaihushi.jphs.nurse.model.NurseActive">
