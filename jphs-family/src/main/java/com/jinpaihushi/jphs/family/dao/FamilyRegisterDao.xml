	<sql id="queryWhere">
		<where>
			<if test="id!=null and id!=''">
				AND FR.id LIKE CONCAT('%', #{id}, '%')
			</if>
			<if test="type!=null">
				AND FR.type=#{type}
			</if>
			<if test="hospital!=null and hospital!=''">
